{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "target": "es5",
    "types": [
      "node"
    ],
    "sourceMap": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
